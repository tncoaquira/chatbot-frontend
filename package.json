{
  "name": "nextjs-typescript",
  "version": "1.11.1",
  "description": "Frontend base con Next.js, MUI v5 y TypeScript",
  "author": "Imanol",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "preinstall": "mkdir -p node_modules; touch ./node_modules/.metadata_never_index",
    "release": "standard-version",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:e2e": "playwright test",
    "test:codegen": "npx playwright codegen -b chromium",
    "test:codegen:mobile": "npx playwright codegen -b chromium --device='iPhone 13'"
  },
